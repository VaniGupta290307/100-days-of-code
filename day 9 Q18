#include<stdio.h>
int main()
{
    int percentage;
    printf("enter percentage ");
    scanf("%d",&percentage);
    if(percentage >= 90 && percentage <100)
    {
        printf("Grade is A");
    }
    else if(percentage >=80 && percentage <=89)
    {
        printf("Grade is B");
    }
    else if(percentage >=70 && percentage <=79)
    {
        printf("Grade is C");
    }
    else if(percentage >=60 && percentage <=69 )
    {
        printf("Grade is D");
    }
    else
    {
        printf("Grade is F");
    }
    return 0;
}
