#include<stdio.h>
void main()
{
    int n,r,sum=0,num;
    printf("Enter value of number");
    scanf("%d",&n);
    num=n;
    while(n>0)
    {
        r=n%10;
        sum=sum+r*r*r;
        n=n/10;
        
    }
    if(sum=num)
    {
        printf("armstrong");
    }
    else
    printf("Not armstrong");
}
