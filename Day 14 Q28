#include<stdio.h>
int main()
{
    int a,n,product=1;
    printf("Enter last number upto which user want");
    scanf("%d",&n);
    for(a=1;a<=n;a++)
    {
        if(a%2==0)
        {
            product=a*product;
        }
    }
    printf("Product of even nummbers from 1 to %d is %d",n,product);
    return 0;
}
